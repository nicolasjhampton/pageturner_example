  "devDependencies": {
    "babel-core": "^6.9.1",
    "babel-loader": "^6.2.4",
    "babel-preset-es2015": "^6.9.0",
    "chai": "^3.5.0",
    "mocha": "^2.5.3",
    "mocha-loader": "^0.7.1",
    "webpack": "^1.13.1",
    "webpack-dev-server": "^1.14.1"
  },
